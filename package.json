{
  "name": "competition-factory-server",
  "private": true,
  "version": "1.1.1",
  "description": "Server side factory mutations",
  "license": "MIT",
  "scripts": {
    "build": "rimraf build && nest -p tsconfig.build.json build",
    "commitlint": "commitlint --edit",
    "commits": "git log $(git describe --tags --abbrev=0)..HEAD --oneline --no-merges --pretty=format:'%s: %h' | sed '/competition-factory-server/d'",
    "build:alpha": "pnpm run commits && pnpm version $(semver $npm_package_version -i premajor --preid alpha) && pnpm build",
    "build:beta": "pnpm run commits && pnpm version $(semver $npm_package_version -i prerelease --preid beta) && pnpm build",
    "minor:beta": "pnpm run commits && pnpm version $(semver $npm_package_version -i preminor --preid beta) && pnpm build",
    "patch:beta": "pnpm run commits && pnpm version $(semver $npm_package_version -i prerelease --preid beta) && pnpm build",
    "build:major": "pnpm run commits && pnpm version $(semver $npm_package_version -i major) && pnpm build",
    "build:minor": "pnpm run commits && pnpm version $(semver $npm_package_version -i minor) && pnpm build",
    "build:patch": "pnpm run commits && pnpm version $(semver $npm_package_version -i patch) && pnpm build",
    "pushbuild": "cp -r build/* dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=production nest start",
    "watch": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=test nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint:staged": "TZ=UTC lint-staged",
    "lint:tsc": "tsc --project tsconfig.json",
    "lint": "eslint src --fix --cache",
    "lint:report": "eslint src & eslint --cache src -f json > eslint-report.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "check-types": "tsc --noEmit",
    "hive-db": "net-level-server",
    "prepare": "husky"
  },
  "dependencies": {
    "@gridspace/net-level": "0.2.4",
    "@gridspace/net-level-client": "0.2.3",
    "@nestjs/cache-manager": "2.2.2",
    "@nestjs/class-transformer": "0.4.0",
    "@nestjs/class-validator": "0.13.4",
    "@nestjs/common": "10.3.7",
    "@nestjs/config": "3.2.1",
    "@nestjs/core": "10.3.7",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/platform-express": "10.3.7",
    "@nestjs/platform-socket.io": "10.3.7",
    "@nestjs/serve-static": "4.0.2",
    "@nestjs/swagger": "7.3.1",
    "@nestjs/websockets": "10.3.7",
    "@next/env": "14.1.4",
    "bcryptjs": "2.4.3",
    "body-parser": "1.20.2",
    "cache-manager": "5.4.0",
    "cache-manager-redis-yet": "4.2.0",
    "dotenv": "^16.4.4",
    "ejs": "3.1.9",
    "form-data": "4.0.0",
    "fs-extra": "11.2.0",
    "mailgun.js": "10.2.1",
    "minimist": "1.2.8",
    "redis": "4.6.13",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "socket.io": "4.7.5",
    "tods-competition-factory": "2.1.5",
    "tods-tmx-classic-converter": "3.0.5",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.7",
    "@swc/cli": "0.3.12",
    "@swc/core": "1.4.11",
    "@types/cache-manager-redis-store": "2.0.4",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.2",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "@typescript-eslint/parser": "7.4.0",
    "compression": "^1.7.4",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "^9.0.11",
    "jest": "29.7.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "rimraf": "^5.0.5",
    "semver": "^7.6.0",
    "source-map-support": "0.5.21",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.3"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*": "prettier --ignore-unknown --write",
    "**/*.ts": "tsc --noEmit"
  },
  "jest": {
    "globalSetup": "./src/tests/config/setup.ts",
    "globalTeardown": "./src/tests/config/teardown.ts",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
