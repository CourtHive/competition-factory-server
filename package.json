{
  "name": "competition-factory-server",
  "private": true,
  "version": "1.0.0",
  "description": "Server side factory mutations",
  "license": "MIT",
  "scripts": {
    "build": "nest -p tsconfig.build.json build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=production nest start",
    "watch": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=test nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint:staged": "TZ=UTC lint-staged",
    "lint:tsc": "tsc --project tsconfig.json",
    "lint": "eslint src --fix --cache",
    "lint:report": "eslint src & eslint --cache src -f json > eslint-report.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.json",
    "check-types": "tsc --noEmit",
    "hive-db": "net-level-server"
  },
  "dependencies": {
    "@gridspace/net-level": "^0.1.10",
    "@gridspace/net-level-client": "^0.1.10",
    "@nestjs/cache-manager": "^2.2.0",
    "@nestjs/class-transformer": "0.4.0",
    "@nestjs/class-validator": "0.13.4",
    "@nestjs/common": "10.3.2",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "10.3.2",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/platform-express": "10.3.2",
    "@nestjs/platform-socket.io": "10.3.2",
    "@nestjs/serve-static": "4.0.1",
    "@nestjs/swagger": "7.3.0",
    "@nestjs/websockets": "10.3.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "1.20.2",
    "cache-manager": "^5.4.0",
    "cache-manager-redis-yet": "^4.1.2",
    "ejs": "^3.1.9",
    "form-data": "^4.0.0",
    "fs-extra": "11.2.0",
    "mailgun.js": "^10.1.0",
    "redis": "^4.6.12",
    "reflect-metadata": "0.2.1",
    "rxjs": "7.8.1",
    "socket.io": "4.7.4",
    "tods-competition-factory": "2.0.23",
    "tods-tmx-classic-converter": "^2.14.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.1",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.3.0",
    "@swc/cli": "^0.3.0",
    "@swc/core": "^1.3.64",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "6.3.4",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*": "prettier --ignore-unknown --write",
    "**/*.ts": "tsc --noEmit"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
