{
  "name": "competition-factory-server",
  "private": true,
  "version": "1.1.1",
  "description": "Server side factory mutations",
  "license": "MIT",
  "scripts": {
    "build": "rimraf build && nest -p tsconfig.build.json build",
    "commitlint": "commitlint --edit",
    "commits": "git log $(git describe --tags --abbrev=0)..HEAD --oneline --no-merges --pretty=format:'%s: %h' | sed '/competition-factory-server/d'",
    "build:alpha": "pnpm run commits && pnpm version $(semver $npm_package_version -i premajor --preid alpha) && pnpm build",
    "build:beta": "pnpm run commits && pnpm version $(semver $npm_package_version -i prerelease --preid beta) && pnpm build",
    "minor:beta": "pnpm run commits && pnpm version $(semver $npm_package_version -i preminor --preid beta) && pnpm build",
    "patch:beta": "pnpm run commits && pnpm version $(semver $npm_package_version -i prerelease --preid beta) && pnpm build",
    "build:major": "pnpm run commits && pnpm version $(semver $npm_package_version -i major) && pnpm build",
    "build:minor": "pnpm run commits && pnpm version $(semver $npm_package_version -i minor) && pnpm build",
    "build:patch": "pnpm run commits && pnpm version $(semver $npm_package_version -i patch) && pnpm build",
    "pushbuild": "cp -r build/* dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=production nest start",
    "watch": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=test nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint:staged": "TZ=UTC lint-staged",
    "lint:tsc": "tsc --project tsconfig.json",
    "lint": "eslint src --fix --cache",
    "lint:report": "eslint src & eslint --cache src -f json > eslint-report.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "check-types": "tsc --noEmit",
    "hive-db": "net-level-server",
    "prepare": "husky"
  },
  "dependencies": {
    "@gridspace/net-level": "0.2.6",
    "@gridspace/net-level-client": "0.2.5",
    "@keyv/redis": "^5.1.3",
    "@nestjs/cache-manager": "3.0.1",
    "@nestjs/common": "11.1.9",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.9",
    "@nestjs/jwt": "11.0.2",
    "@nestjs/platform-express": "11.1.9",
    "@nestjs/platform-socket.io": "11.1.9",
    "@nestjs/serve-static": "5.0.4",
    "@nestjs/swagger": "11.2.3",
    "@nestjs/websockets": "11.1.9",
    "@next/env": "16.1.0",
    "axios": "^1.6.8",
    "bcryptjs": "3.0.3",
    "body-parser": "2.2.1",
    "cache-manager": "6.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "codepage": "^1.15.0",
    "dotenv": "^17.0.0",
    "ejs": "3.1.10",
    "express": "^5.0.0",
    "form-data": "4.0.5",
    "fs-extra": "11.3.2",
    "keyv": "^5.5.3",
    "mailgun.js": "12.4.0",
    "minimist": "1.2.8",
    "node-html-parser": "^7.0.0",
    "redis": "5.10.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "socket.io": "4.8.3",
    "tods-competition-factory": "2.2.34",
    "tods-tmx-classic-converter": "3.0.5",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@nestjs/cli": "11.0.14",
    "@nestjs/schematics": "11.0.9",
    "@nestjs/testing": "11.1.9",
    "@swc/cli": "0.7.9",
    "@swc/core": "1.15.7",
    "@types/express": "5.0.6",
    "@types/jest": "30.0.0",
    "@types/node": "24.10.4",
    "@types/supertest": "6.0.3",
    "@typescript-eslint/eslint-plugin": "8.50.0",
    "@typescript-eslint/parser": "8.50.0",
    "compression": "^1.7.4",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "globals": "^16.0.0",
    "husky": "^9.0.11",
    "jest": "30.2.0",
    "lint-staged": "16.2.7",
    "prettier": "3.7.4",
    "rimraf": "^6.0.0",
    "semver": "^7.6.2",
    "source-map-support": "0.5.21",
    "supertest": "7.1.4",
    "ts-jest": "29.4.6",
    "ts-loader": "9.5.4",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.3"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*": "prettier --ignore-unknown --write",
    "**/*.ts": "tsc --noEmit"
  },
  "pnpm": {
    "overrides": {
      "serve-static@<2": ">=2.2.0"
    }
  },
  "jest": {
    "globalSetup": "./src/tests/config/setup.ts",
    "globalTeardown": "./src/tests/config/teardown.ts",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
